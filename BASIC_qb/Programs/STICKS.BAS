CLS
PRINT "TO WIN YOU MUST TAKE THE LAST STICK"
P = 0
N = 15
FOR Y = 1 TO 15
   PRINT "THERE ARE"; N; "STICKS"
LABEL: PRINT "HOW MANY DO YOU WANT TO TAKE? 1, 2, OR 3"
   INPUT P
   PRINT
   IF (P < 1) OR (P > 3) OR ((N - P) < 0) THEN
      PRINT "NO GOOD, PICK AGAIN"
      GOTO LABEL
   END IF
   N = N - P
   IF N = 0 THEN
      PRINT "YOU WIN!!"
   END
   END IF
NEXT Y


